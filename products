rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
  
  match /feedback/{documentId} {
  allow create: if true;
  allow read, write, update, delete: if false;
  }
    match /{document=**} {
      allow read, write: if
          request.time < timestamp.date(2022, 8, 10);
    }
  }
}